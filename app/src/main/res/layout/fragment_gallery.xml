            android:minHeight="48dp"
            android:text="Nissan"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName4" />

        <EditText
            android:id="@+id/editTextTextPersonName"
            android:layout_width="match_parent"
